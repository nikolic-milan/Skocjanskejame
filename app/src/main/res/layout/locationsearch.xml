<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    tools:context="android.parkskocjanskejame.LocationSearch">

    <android.parkskocjanskejame.utils.CustomTextView
        android:id="@+id/locationsearchText1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        android:fontFamily="s"
        android:gravity="center"
        android:lines="2"
        android:text="@string/iskanjeLokacije"
        android:textColor="@color/textColor"
        android:textSize="35dp"
        android:typeface="normal"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"/>

    <!--android.parkskocjanskejame.utils.CustomTextView
        android:id="@+id/locationsearchText1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/iskanjeLokacije"
        android:textColor="@color/textColor"
        android:textSize="35dp"
        android:gravity="center"
        android:layout_marginTop="37dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" /-->

    <android.parkskocjanskejame.utils.CustomTextView
        android:id="@+id/locationsearchText2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/progressbar"
        android:fontFamily="s"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="@string/locationSearch2"
        android:textColor="@color/textColor"
        android:textSize="28dp"
        android:typeface="normal" />


    <ProgressBar
        android:id="@+id/progressbar"
        style="?android:attr/progressBarStyleLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/locationsearchText1"
        android:layout_centerHorizontal="true"
        android:indeterminate="true"
        android:layout_margin="40dp"
        android:indeterminateDrawable="@drawable/customprogressbar" />

    <ImageView
        android:id="@+id/help"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="5dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        app:srcCompat="@drawable/help" />


</RelativeLayout>
